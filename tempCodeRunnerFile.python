class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
    def slinkedlist(self):
        self.head = None
    def reverselist(self):
        prev = None
        current = self.head
        while current:
            nxt = current.next
            current.next = prev
            prev = current
            current = nxt
        return prev
